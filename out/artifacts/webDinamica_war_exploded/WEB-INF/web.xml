    <servlet>
        <servlet-name>ControladorCategoria</servlet-name>
        <servlet-class>controlador.ControladorCategoria</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorPlantilla</servlet-name>
        <servlet-class>controlador.ControladorPlantilla</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorLogin</servlet-name>
        <servlet-class>controlador.ControladorLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorGoogle</servlet-name>
